<section id="accomodations">
    <% const accomodations = data('accomodations') %>
    <section class="container accomodations-container">
    <% for (const accomodation of accomodations.filter((acc) => !acc.popular)) {%>
        <%- include(template('accomodations/accomodation'), { accomodation, classes: 'card horizontal' }) %>
        <% } %>
    </section>
    <aside class="container popular-container">
        <% for (const accomodation of accomodations.filter((acc) => acc.popular)) {%>
            <%- include(template('accomodations/accomodation'), { accomodation, classes: 'card vertical' }) %>
            <% } %>
    </aside>
</section>