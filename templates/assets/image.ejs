<picture <%- attributes ? Object.keys(attributes).map((key) => `${key}="${attributes[key]}"`).join(' ') : null %> >
    <source media="(max-width:768px)" srcset="<%- image(collection, thumbnail, 'sm') %>"/>
    <source media="(max-width:992px) and (min-width:768px)" srcset="<%- image(collection, thumbnail, 'md') %>"/>
    <img src="<%- image(collection, thumbnail, 'lg') %>" alt="<%= alt %>"/>
</picture>
